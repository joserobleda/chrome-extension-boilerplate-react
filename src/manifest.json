{
  "manifest_version": 3,
  "name": "NurtureIn",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "contentScript.bundle.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": []
    }
  ],
  "permissions": [
    "cookies",
    "storage",
    "alarms",
    "tabs"
  ],
  "host_permissions": [
    "*://*.linkedin.com/*",
    "https://retargetin.vercel.app/*"
  ]
}